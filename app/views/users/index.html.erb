<h2> All Users</h2>

<%= form_tag users_path, method: :get do %>
  <%= text_field_tag 'term', params[:term], placeholder: "Enter username" %>
  <%= submit_tag 'Search' %>
<% end %>



<ul class="users">
  <% @users.each do |user| %>
    <li>
       <%= link_to user.user_name, profile_path(user.user_name) %> 
        
        
    </li>
  <% end %>
</ul>

<%= link_to "Cancel", root_path(@post) %>